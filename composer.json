{
  "name": "plaisio/helper-html",
  "description": "PhpPlaisio: HTML",
  "keywords": [
    "PhpPlaisio",
    "HTML"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "ext-mbstring": "*",
    "setbased/exception": "^2.0.0",
    "setbased/helper-cast": "^2.0.0"
  },
  "require-dev": {
    "ext-dom": "*",
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Helper\\": "src/"
    }
  },
  "autoload-dev": {
