{
  "name": "plaisio/helper-html",
  "description": "PhpPlaisio: HTML",
  "keywords": [
    "PhpPlaisio",
    "HTML"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-mbstring": "*",
    "setbased/exception": "^2.6.0"
  },
  "require-dev": {
    "ext-dom": "*",
    "phing/phing": "^3.0.1",
    "phpunit/phpunit": "^11.5.22",
    "setbased/error-handler": "^1.4.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Helper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Helper\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
